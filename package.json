{
  "name": "zip-mcp",
  "version": "1.0.3",
  "description": "A Model Context Protocol server for ZIP compression using fastMCP and zip.js",
  "main": "dist/index.js",
  "bin": {
    "zip-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "npx tsx src/index.ts",
    "build": "tsc && node scripts/add-shebang.js && chmod +x dist/index.js",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@zip.js/zip.js": "^2.7.57",
    "fastmcp": "^1.20.5"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7gugu/zip-mcp.git"
  },
  "keywords": [
    "mcp",
    "server",
    "typescript",
    "compression",
    "zip"
  ],
  "author": "7gugu",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module"
}
